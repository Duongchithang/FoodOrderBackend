{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "Category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "content": {
      "type": "richtext"
    },
    "avatar": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "Media": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "index": {
      "type": "integer"
    },
    "price": {
      "type": "biginteger"
    },
    "big_categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::big-category.big-category",
      "inversedBy": "categories"
    }
  }
}
